<div class="responsive">

    <h4>Waterfall Project details:</h4>

    <form action="/editWaterfallActivity/<%= tenantData[0] %>" method="post">
        <div class="responsive">

            <table class="table">
                <thead>
                <tr>

                    <% for(var i = 0; i < tenantFields.length; i++){ %>
                    <th><%= tenantFields[i] %></th>
                    <% } %>

                </tr>
                </thead>

                <tbody>
                <tr>
                    <% for(var i = 0; i < tenantFields.length; i++) { %>
                    <td>


                        <div>
                            <input type="text" id="<%= tenantFields[i] %>" name="<%= tenantFields[i] %>"
                                   value="<%= tenantData[i] %>">
                        </div>
                    </td>
                    <% } %>

                </tr>


                </tbody>
            </table>
            <div style="float: right; margin-right: 50px;">
                <br>
                <button class="btn btn-action btn-sm pull-right" type="submit">Update</button>
            </div>
        </div>
    </form>
